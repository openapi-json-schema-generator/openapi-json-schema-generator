{{#if allOf}}

class AllOf:
{{#each allOf}}
{{#if refInfo.refClass}}

    {{> model_templates/refClassStaticmethod }}
{{else}}
    {{> model_templates/schema }}
{{/if}}
{{/each}}
    classes = [
{{#each allOf}}
    {{#if refInfo.refClass}}
        {{name.getSnakeCaseName}},
    {{else}}
        {{name.getCamelCaseName}},
    {{/if}}
{{/each}}
    ]
{{/if}}
