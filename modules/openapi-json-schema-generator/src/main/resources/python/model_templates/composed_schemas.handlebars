{{#if allOf}}

class all_of:
{{#each allOf}}
{{#unless refClass}}
    {{> model_templates/schema }}
{{else}}

    @staticmethod
    def {{#if nameInSnakeCase}}{{name}}{{else}}{{baseName}}{{/if}}() -> typing.Type['{{refClass}}']:
        return {{refClass}}
{{/unless}}
{{/each}}
    classes = [
{{#each allOf}}
    {{#if nameInSnakeCase}}
        {{name}},
    {{else}}
        {{baseName}},
    {{/if}}
{{/each}}
    ]
{{/if}}
