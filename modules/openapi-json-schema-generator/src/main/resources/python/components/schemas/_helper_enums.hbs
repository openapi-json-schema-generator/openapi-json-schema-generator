{{#if enumNameToValue}}
{{#each enumNameToValue}}

@schemas.classproperty
def {{@key}}(cls):
    {{#eq this.type "string"}}
    return cls("{{{this.value}}}")
    {{/eq}}
    {{#eq this.type "number"}}
    return cls({{{this.value}}})
    {{/eq}}
    {{#eq this.type "integer"}}
    return cls({{{this.value}}})
    {{/eq}}
    {{#eq this.type "boolean"}}
        {{#if this.value}}
    return cls(True)
        {{else}}
    return cls(False)
        {{/if}}
    {{/eq}}
    {{#eq this.type "null"}}
    return cls(None)
    {{/eq}}
{{/each}}
{{/if}}