{{#if not}}
{{#with not}}

@staticmethod
def not_():
    {{#if refInfo.refClass}}
    return {{#if refInfo.refModule}}{{refInfo.refModule}}.{{/if}}{{refInfo.refClass}}
    {{else}}
    return {{jsonPathPiece.camelCase}}
    {{/if}}
{{/with}}
{{/if}}
