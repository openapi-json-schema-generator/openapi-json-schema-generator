{{#if allOf}}

class AllOf:
{{#each allOf}}
{{#if refInfo.refClass}}

    {{> components/schemas/_helper_refclass_staticmethod }}
{{else}}
    {{> components/schemas/_helper_schema_switch_case }}
{{/if}}
{{/each}}
    classes = [
{{#each allOf}}
    {{#if refInfo.refClass}}
        {{name.snakeCase}},
    {{else}}
        {{name.camelCaseName}},
    {{/if}}
{{/each}}
    ]
{{/if}}
