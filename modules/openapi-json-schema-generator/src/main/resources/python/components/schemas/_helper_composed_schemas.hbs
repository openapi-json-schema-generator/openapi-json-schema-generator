{{#if allOf}}

@staticmethod
def all_of():
    return (
    {{#each allOf}}
        {{#if refInfo.refClass}}
        {{#if refInfo.refModule}}{{refInfo.refModule}}.{{/if}}{{refInfo.refClass}},
        {{else}}
        {{jsonPathPiece.camelCase}},
        {{/if}}
    {{/each}}
    )
{{/if}}
