enum_value_to_name = {
{{#each enumValueToName}}
    {{#eq @key.type "string"}}
    "{{{@key.value}}}": "{{this}}",
    {{/eq}}
    {{#eq @key.type "number"}}
    {{{@key.value}}}: "{{this}}",
    {{/eq}}
    {{#eq @key.type "integer"}}
    {{{@key.value}}}: "{{this}}",
    {{/eq}}
    {{#eq @key.type "boolean"}}
        {{#if @key.value}}
    schemas.BoolClass.TRUE: "{{this}}",
        {{else}}
    schemas.BoolClass.FALSE: "{{this}}",
        {{/if}}
    {{/eq}}
    {{#eq @key.type "null"}}
    schemas.NoneClass.NONE: "{{this}}",
    {{/eq}}
{{/each}}
}
