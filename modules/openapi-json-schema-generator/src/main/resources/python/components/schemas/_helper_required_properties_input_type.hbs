{{#if additionalProperties}}
    {{#if additionalProperties.isBooleanSchemaFalse}}
        {{! TypedDict use case }}
{{requiredProperties.jsonPathPiece.camelCase}} = typing_extensions.TypedDict(
    '{{requiredProperties.jsonPathPiece.camelCase}}',
    {
        {{#each requiredProperties}}
            {{#with this}}
                {{#if refInfo.refClass}}
        {{> components/schemas/_helper_prefix_ref_property_value_type prefix=@key.original fullRefModule="" }}
                {{else}}
                    {{#if jsonPathPiece}}
        {{> components/schemas/_helper_prefix_property_value_type prefix=@key.original }}
                    {{else}}
        "{{{@key.original}}}": typing.Union[
            schemas.AnyTypeSchema,
            {{> _helper_schema_python_types_newline }}
        ],
                    {{/if}}
                {{/if}}
            {{/with}}
        {{/each}}
    }
)
    {{else}}
        {{! additionalProperties True/schema }}
        {{#if additionalProperties.isBooleanSchemaTrue }}
            {{! addProps True, does not matter what properties are because they are always covered by base types }}
{{requiredProperties.jsonPathPiece.camelCase}} = typing.Mapping[str, schemas.INPUT_TYPES_ALL_INCL_SCHEMA]
        {{else}}
            {{! addProps schema }}
{{requiredProperties.jsonPathPiece.camelCase}} = typing.Mapping[
    str,
    typing.Union[
        {{#each requiredProperties}}
            {{#with this}}
        {{> components/schemas/_helper_property_value_type }}
            {{/with}}
        {{/each}}
        {{#with additionalProperties}}
        {{> components/schemas/_helper_property_value_type }}
        {{/with}}
    ]
]
        {{/if}}
    {{/if}}
{{else}}
    {{! addProps unset, does not matter what required props are }}
{{requiredProperties.jsonPathPiece.camelCase}} = typing.Mapping[str, schemas.INPUT_TYPES_ALL_INCL_SCHEMA]
{{/if}}
