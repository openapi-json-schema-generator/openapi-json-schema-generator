{{#if types}}
types: typing.FrozenSet[typing.Type] = frozenset({
{{#each types}}
{{#eq this "null"}}
    schemas.none_type_,
{{/eq}}
{{#eq this "object"}}
    schemas.immutabledict,
{{/eq}}
{{#eq this "array"}}
    tuple,
{{/eq}}
{{#eq this "string" }}
    {{#eq ../format "binary"}}
    FileIO,
    bytes,
    {{else}}
    str,
    {{/eq}}
{{/eq}}
{{#eq this "integer"}}
    int,
{{/eq}}
{{#eq this "number"}}
    float,
    int,
{{/eq}}
{{#eq this "boolean"}}
    schemas.Bool,
{{/eq}}
{{/each}}
})
{{/if}}