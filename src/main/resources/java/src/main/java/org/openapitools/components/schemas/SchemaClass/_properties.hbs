static final LinkedHashMap<String, Class<?>> properties = new LinkedHashMap<>(Map.ofEntries(
{{#each properties}}
    {{#if refInfo.refClass}}
        {{#if refInfo.refModule}}
    new AbstractMap.SimpleEntry<String, Class<?>>("{{{@key.original}}}", {{refInfo.refModule}}.{{refInfo.refClass}}.class){{#unless @last}},{{/unless}}
        {{else}}
    new AbstractMap.SimpleEntry<String, Class<?>>("{{{@key.original}}}", {{refInfo.refClass}}.class){{#unless @last}},{{/unless}}
        {{/if}}
    {{else}}
    new AbstractMap.SimpleEntry<String, Class<?>>("{{{@key.original}}}", {{jsonPathPiece.camelCase}}.class){{#unless @last}},{{/unless}}
    {{/if}}
{{/each}}
));
