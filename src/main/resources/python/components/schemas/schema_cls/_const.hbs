const_value_to_name: typing.Mapping[typing.Union[int, float, str, schemas.Bool, None], str] = dataclasses.field(
    default_factory=lambda: {
{{#each constInfo.valueToName}}
    {{#eq @key.type "string"}}
        "{{{@key.value}}}": "{{this}}",
    {{/eq}}
    {{#eq @key.type "number"}}
        {{{@key.value}}}: "{{this}}",
    {{/eq}}
    {{#eq @key.type "integer"}}
        {{{@key.value}}}: "{{this}}",
    {{/eq}}
    {{#eq @key.type "boolean"}}
        {{#if @key.value}}
        schemas.Bool.TRUE: "{{this}}",
        {{else}}
        schemas.Bool.FALSE: "{{this}}",
        {{/if}}
    {{/eq}}
    {{#eq @key.type "null"}}
        None: "{{this}}",
    {{/eq}}
{{/each}}
    }
)
