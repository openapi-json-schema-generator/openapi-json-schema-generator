{{#if refInfo}}
    {{#or refInfo.refModuleAlias refInfo.refModule}}
        {{#with refInfo.ref}}
            {{#eq fullRefModule "" }}
                {{#if ../refInfo.refModuleAlias}}
{{> components/schemas/_helper_schema_io_type fullRefModule=../refInfo.refModuleAlias }}
                {{else}}
{{> components/schemas/_helper_schema_io_type fullRefModule=../refInfo.refModule }}
                {{/if}}
            {{else}}
                {{#if ../refInfo.refModuleAlias}}
{{> components/schemas/_helper_schema_io_type fullRefModule=(join fullRefModule ../refInfo.refModuleAlias ".") }}
                {{else}}
{{> components/schemas/_helper_schema_io_type fullRefModule=(join fullRefModule ../refInfo.refModule ".") }}
                {{/if}}
            {{/eq}}
        {{/with}}
    {{else}}
        {{! self reference }}
        {{#with refInfo.ref}}
{{> components/schemas/_helper_schema_io_type }}
        {{/with}}
    {{/or}}
{{else}}
    {{! no more refs }}
    {{#if types}}
        {{#eq types.size 1}}
{{> components/schemas/_helper_schema_io_type_base }}
        {{else}}
{{paramName}}: typing.Union[
    {{> components/schemas/_helper_schema_io_type_base endChar="," paramName=null }}
]{{endChar}}
        {{/eq}}
    {{else}}
{{> components/schemas/_helper_schema_io_type_base }}
    {{/if}}
{{/if}}