{{arrayInputJsonPathPiece.pascalCase}} = typing.Union[
    typing.List[
    {{#with listItemSchema}}
        {{> components/schemas/types/schema_io_type paramName=false fullRefModule="" endChar="," rootClass=../arrayInputJsonPathPiece.pascalCase }}
    {{/with}}
    ],
    typing.Tuple[
    {{#if prefixItems}}
        {{#each prefixItems}}
        {{> components/schemas/types/schema_io_type paramName=false fullRefModule="" endChar="," rootClass=../arrayInputJsonPathPiece.pascalCase }}
        {{/each}}
        {{#with items}}
        typing_extensions.Unpack[typing.Tuple[
            {{> components/schemas/types/schema_io_type paramName=false fullRefModule="" endChar="," rootClass=../arrayInputJsonPathPiece.pascalCase }}
            ...
        ]]
        {{else}}
        typing_extensions.Unpack[typing.Tuple[schemas.INPUT_TYPES_ALL, ...]]
        {{/with}}
    {{else}}
        {{#with items}}
        {{> components/schemas/types/schema_io_type paramName=false fullRefModule="" endChar="," rootClass=../arrayInputJsonPathPiece.pascalCase }}
        {{/with}}
        ...
    {{/if}}
    ]
]
