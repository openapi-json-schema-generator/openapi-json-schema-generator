{{#if forDocs}}
oneOf = List.of(<br>
    {{~#each oneOf}}
        {{#if refInfo.refClass}}
            {{#if refInfo.refModule}}
&nbsp;&nbsp;&nbsp;&nbsp;[{{refInfo.refModule}}.{{refInfo.refClass}}.class]({{docRoot}}{{refInfo.ref.pathFromDocRoot}}.md#{{refInfo.ref.jsonPathPiece.kebabCase}}){{#unless @last}},{{/unless}}<br>
            {{~else}}
&nbsp;&nbsp;&nbsp;&nbsp;[{{refInfo.refClass}}.class](#{{refInfo.ref.jsonPathPiece.kebabCase}}){{#unless @last}},{{/unless}}<br>
            {{~/if}}
        {{else}}
&nbsp;&nbsp;&nbsp;&nbsp;[{{jsonPathPiece.pascalCase}}.class](#{{jsonPathPiece.kebabCase}}){{#unless @last}},{{/unless}}<br>
        {{~/if}}
    {{/each}}
))<br>
{{~else}}
.oneOf(List.of(
    {{#each oneOf}}
        {{#if refInfo.refClass}}
            {{#if refInfo.refModule}}
    {{refInfo.refModule}}.{{refInfo.refClass}}.class{{#unless @last}},{{/unless}}
            {{else}}
    {{refInfo.refClass}}.class{{#unless @last}},{{/unless}}
            {{/if}}
        {{else}}
    {{jsonPathPiece.pascalCase}}.class{{#unless @last}},{{/unless}}
        {{/if}}
    {{/each}}
))
{{/if}}